    #include<bits/stdc++.h>
    #define bst           ios_base::sync_with_stdio(0);
    #define bug(x)        cerr<<#x<<'='<<x<<endl;
    #define lp(i,a,b)     for(int i=a; i<=b; i++)
    #define ll            long long int
    #define pb            emplace_back
    using namespace std;
    int main(){
        bst
        ll t, i,j , k;
        cin>>t;
        for(i=0; i<t; i++){
            ll n;
            cin>>n;
            ll keep=n%14;
            if(keep>0 && keep<7 && n>=15) cout<<"YES"<<endl;
            else cout<<"NO"<<endl;
        }
        
        return 0;
    }
