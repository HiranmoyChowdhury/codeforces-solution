    #include<bits/stdc++.h>
    #define bst           ios_base::sync_with_stdio(0);
    #define bug(x)        cerr<<#x<<'='<<x<<endl;
    #define lp(i,a,b)     for(int i=a; i<=b; i++)
    #define ll            long long int
    #define pb            emplace_back
    using namespace std;
    int main(){
        ll t, mod=1000000007;
        cin>>t;
        while(t--){
            string s;
            cin>>s;
            ll i, j, k, siz=s.size()-1;
            if(s[siz]=='o') printf("FILIPINO\n");
            else if(s[siz]=='u') printf("JAPANESE\n");
            else printf("KOREAN\n");
        }
        
        return 0;
    }
